cd /d %~dp0
call 0000_setEnv.bat
setlocal enabledelayedexpansion
set sql=
for %%t in (%MASTER_TABLES% %DATA_TABLES%) do (
  set sql=!sql!truncate table %SCHEMA%.%%t;
  set sql=!sql!insert into %SCHEMA%.%%t select * from csvread('%CSVDIR%/%%t.csv', null, 'UTF-8', char(9^), '''', ''''^);
  for %%g in (%ID_GEN_TABLES%) do (
    if %%t==%%g set sql=!sql!alter table %SCHEMA%.%%t alter column id set generated by default restart with (select max(id^)+1 from %SCHEMA%.%%t^);
  )
)
"%JAVAEXE%" -cp "%H2DRIVERS%;%CLASSPATH%" org.h2.tools.Shell -url "%H2URL%" -user "%H2USR%" -password "%H2PWD%" -sql "%sql%"
pause
